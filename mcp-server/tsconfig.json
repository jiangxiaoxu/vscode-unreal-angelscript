{
    "compilerOptions": {
        "target": "es2020",
        "lib": ["ES2020"],
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": false,
        "outDir": "out",
        "rootDir": "src",
        "strictNullChecks": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": true,
        "noImplicitAny": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
